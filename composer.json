{
    "name": "jaacu/laravel-model-hashids",
    "description": "",
    "keywords": [
        "jaacu",
        "laravel-model-hashids"
    ],
    "homepage": "https://github.com/jaacu/laravel-model-hashids",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Javier Cabello",
            "email": "jaacu.97@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.8.*",
        "vinkla/hashids": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "3.8.*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Jaacu\\LaravelModelHashids\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jaacu\\LaravelModelHashids\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jaacu\\LaravelModelHashids\\LaravelModelHashidsServiceProvider"
            ],
            "aliases": {
                "LaravelModelHashids": "Jaacu\\LaravelModelHashids\\LaravelModelHashidsFacade"
            }
        }
    }
}
